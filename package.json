{
  "name": "jsonlint-pro",
  "version": "0.0.0",
  "description": "Jsonlint website",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test": "",
    "start": "npm run serve-prod",
    "serve-prod": "PORT=5001 pm2 startOrRestart ecosystem.json --env production --watch",
    "serve-prod-api-only": "API_ONLY=yes npm run serve",
    "serve-dev": "opn http://localhost:5001; PORT=5001 NODE_ENV=development nodemon index.js --ignore 'frontend/*' --ignore 'public/*' --ignore 'node_modules/*' --ignore webpack.config.js --ignore package.json",
    "bundle": "npm run clean; NODE_ENV=production webpack --config ./webpack/webpack.config.babel.js",
    "check-updates": "ncu; ncu --packageFile ./frontend/package.json",
    "upgrade": "ncu -a; npm install; ncu --packageFile ./frontend/package.json -a; cd ./frontend; npm install",
    "npm-frontend": "cd frontend; npm",
    "lint": "eslint . --ext .js --cache --ignore-pattern '**/node_modules/**' --ignore-pattern '**/public/**'",
    "clean": "rimraf public",
    "deploy": "pm2 deploy ecosystem.json production update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circlecell/jsonlint.git"
  },
  "keywords": [
    "jsonlint"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/circlecell/jsonlint/issues"
  },
  "homepage": "https://github.com/circlecell/jsonlint#readme",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "0.3.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.5",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "nodemon": "^1.10.0",
    "npm-check-updates": "^2.7.2",
    "object.assign": "^4.0.4",
    "open-browser-webpack-plugin": "0.0.2",
    "opn-cli": "^3.1.0",
    "postcss-calc": "^5.3.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "postcss-url": "^5.1.2",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-split-by-path": "^0.1.0"
  },
  "dependencies": {
    "aws-sdk": "^2.4.9",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jsonschema": "^1.1.0",
    "md5": "^2.1.0",
    "request": "^2.74.0",
    "valid-url": "^1.0.9"
  },
  "peerDependencies": {
    "pm2": "^1.1.3"
  }
}
