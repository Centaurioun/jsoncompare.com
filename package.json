{
  "name": "jsonlint-pro",
  "version": "0.0.0",
  "description": "Jsonlint website",
  "main": "app.js",
  "scripts": {
    "develop": "npm-run-all --parallel serve watch open-in-browser",
    "serve": "PORT=5001 nodemon app.js --ignore 'frontend/*' --ignore 'public/*' --ignore 'node_modules/*' --ignore webpack.config.js --ignore package.json",
    "open-in-browser": "opn http://localhost:5001",
    "watch": "webpack --watch --progress",
    "bundle": "webpack",
    "check-updates": "ncu",
    "upgrade": "ncu --upgradeAll && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circlecell/jsonlint.git"
  },
  "keywords": [
    "jsonlint"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/circlecell/jsonlint/issues"
  },
  "homepage": "https://github.com/circlecell/jsonlint#readme",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "0.3.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "balajs": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.10.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsonlint": "^1.6.2",
    "nodemon": "^1.9.2",
    "npm-check-updates": "^2.7.0",
    "npm-run-all": "^2.3.0",
    "object.assign": "^4.0.4",
    "opn-cli": "^3.1.0",
    "postcss-calc": "^5.3.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "postcss-url": "^5.1.2",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "aws-sdk": "^2.4.6",
    "babel-polyfill": "^6.9.1",
    "body-parser": "^1.15.2",
    "codemirror": "^5.16.0",
    "diff-match-patch": "^1.0.0",
    "express": "^4.14.0",
    "jsonschema": "^1.1.0",
    "matreshka": "^1.9.1",
    "matreshka-router": "0.0.5",
    "md5": "^2.1.0",
    "request": "^2.73.0",
    "valid-url": "^1.0.9"
  }
}
