{
    "parser": "babel-eslint",
    "extends": [
        "plugin:import/errors",
        "plugin:import/warnings",
        "airbnb"
    ],
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-var": "error",
        "comma-dangle": 0,
        "global-require": 0,
        "import/no-unresolved": 0
    },
    "env": {
        "browser": true
    }
}
